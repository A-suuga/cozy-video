{
  "name": "cozy-video",
  "version": "0.1.0",
  "scripts": {
    "tx": "tx pull --all || true",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint '{src,test}/**/*.{js,jsx}'",
    "lint:styles": "stylint src/styles --config ./.stylintrc",
    "prebuild": "yarn lint",
    "build": "yarn build:browser",
    "build:browser": "cozy-scripts build --browser",
    "build:mobile": "cozy-scripts build --mobile",
    "watch": "yarn watch:browser",
    "watch:browser": "cozy-scripts watch --browser",
    "watch:mobile": "cozy-scripts watch --mobile",
    "start": "cozy-scripts start --hot --browser",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/Asuuga/cozy-video.git}",
    "pretest": "yarn lint",
    "test": "cozy-scripts test --verbose --coverage",
    "cozyPublish": "git fetch origin ${DEPLOY_BRANCH:-build}:${DEPLOY_BRANCH:-build} && cozy-scripts publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})",
    "buildShaka": "if [ ! -d  'node_modules/shaka-player/dist' ]; then mkdir 'node_modules/shaka-player/dist'; fi; python node_modules/shaka-player/build/build.py --nodemo +@complete -@offline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Asuuga/cozy-video.git"
  },
  "author": "",
  "contributors": [],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/Asuuga/cozy-video/issues"
  },
  "homepage": "https://github.com/Asuuga/cozy-video#readme",
  "devDependencies": {
    "babel-preset-cozy-app": "1.3.2",
    "cozy-bar": "6.10.2",
    "cozy-client-js": "0.14.2",
    "cozy-ui": "18.12.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.5.0",
    "eslint": "5.14.1",
    "eslint-config-cozy-app": "1.1.8",
    "eslint-config-prettier": "4.1.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "git-directory-deploy": "1.5.1",
    "prettier": "1.14.3",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-hot-loader": "4.3.11",
    "react-test-renderer": "16.5.2",
    "redux-mock-store": "1.5.3",
    "stylint": "1.5.9",
    "webpack": "^4.29.5"
  },
  "dependencies": {
    "cozy-client": "6.3.3",
    "cozy-scripts": "1.12.2",
    "piwik-react-router": "0.12.1",
    "preact": "8.3.1",
    "preact-compat": "3.18.4",
    "preact-portal": "1.1.3",
    "react-redux": "5.1.1",
    "react-router-dom": "4.3.1",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "shaka-player": "A-suuga/shaka-player#cozy",
    "stylint": "1.5.9",
    "stylus": "0.54.5"
  }
}
